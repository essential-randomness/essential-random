{
	"version": 3,
	"routes": [
		{
			"src": "/subscribe",
			"headers": {
				"Location": "/support-me"
			},
			"status": 301
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/posts\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/subscribe\\/?$",
			"dest": "_render"
		}
	]
}